{
    "name": "andreger/laravel-bootstrap",
    "description": "This library provides a collection of Bootstrap components.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "André Gervásio",
            "email": "andreger@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Andreger\\Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andreger\\Bootstrap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreger\\Bootstrap\\BootstrapServiceProvider"
            ]
        }
    }
}
